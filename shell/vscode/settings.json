{
  "files.associations": {
    "*.tpl": "php",
    "*.js": "javascript"
  },
  "vetur.validation.template": false,
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.autoFixOnSave": true,
  "editor.formatOnSave": false,
  "breadcrumbs.enabled": true,
  "editor.fontFamily": "'monospace', 'Hack', 'Droid Sans Mono', 'Inconsolata'",
  "workbench.editor.highlightModifiedTabs": true,
  "//": "plugin ---> esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[sql]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "// tor.experimental.stickyScroll.enabled": "function name 浮動停留在第一行",
  "editor.codeActionsOnSave": {
    "//": "是否儲存時自動 formatter",
    "source.fixAll.eslint": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "// editor.wordWrap": "自動換行 on, 不要換行 off",
  "editor.wordWrap": "off",
  "// editor.useTabStops": "貼上內容後, 原本是 tab 的內容轉換成 space",
  "//// editor.useTabStops": true,
  "editor.insertSpaces": true,
  "hediet.vscode-drawio.local-storage": "",
  "workbench.editor.untitled.hint": "hidden",
  "plantuml.render": "PlantUMLServer",
  "cSpell.userWords": [
    "loggly"
  ],
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "emmet.excludeLanguages": [
    "markdown",
    "php"
  ],
  "editor.stickyScroll.enabled": true,
  "window.zoomLevel": 1
}


